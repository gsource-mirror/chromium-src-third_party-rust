import("//build/rust/rust_static_library.gni")

rust_static_library("jxl_rs") {
  crate_root = "lib.rs"
  crate_name = "jxl_rs"
  edition = "2021"
  allow_unsafe = true

  sources = [ "lib.rs" ]
  cxx_bindings = [ "lib.rs" ]

  deps = [
    "..:lib",
    "//build/rust:cxx_cppdeps",
  ]
}
